<config>
    <cloudurl>edudocs.de/oer</cloudurl> <!-- CHANGE THIS to your cloud domain, e.g. mycloud.mydomain.org or www.mydomain.org/mycloud (without https://) -->
    <adminname>Cloudmanager</adminname> <!-- CHANGE THIS to your cloud user, who has admin permissions-->
    <adminpass>yourPass</adminpass> <!-- CHANGE THIS to the password for that user -->
	<csvdelimiter>;</csvdelimiter> <!-- change this to ; if you create your CSV-file with Apple Numbers -->
	<csvdelimitergroups>,</csvdelimitergroups> <!-- change this to , if you create your CSV-file with Apple Numbers -->
</config>
